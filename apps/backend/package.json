{
  "name": "@ddangha/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start": "uvicorn main:app --host 0.0.0.0 --port 8000",
    "lint": "ruff check .",
    "format": "ruff format .",
    "type-check": "mypy .",
    "test": "python -m pytest",
    "test:watch": "python -m pytest --watch",
    "test:cov": "python -m pytest --cov=. --cov-report=html --cov-report=term",
    "test:verbose": "python -m pytest -v",
    "test:unit": "python -m pytest -m unit",
    "test:integration": "python -m pytest -m integration"
  },
  "dependencies": {}
}
